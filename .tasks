# vim: set fenc=utf-8 ft=dosini:
# see: https://github.com/skywind3000/asynctasks.vim/wiki/Task-Config

[project-init]
command=mkdir -p build && cd build && cmake ..
cwd=<root>

[project-build]
command=./build.sh
cwd=<root>
errorformat=%. %#--> %f:%l:%c
save=1

[project-run]
; command=build/pcm_trans -i=./tips/dd1.pcm -o=./tips2/dd1.pcm
command=http
cwd=<root>
output=quickfix

